extends: airbnb
rules:
  max-len: [1, 120, 4]
  no-multi-spaces: 0
  react/require-extension: 0
  react/forbid-prop-types: 0
  react/jsx-filename-extension: [1, { extensions: [.test.js, .jsx] }]
  import/extensions: 0
  react/require-default-props: 0
globals:
  document: true
  foo: true
  window: true
  describe: true
  it: true
  expect: true
